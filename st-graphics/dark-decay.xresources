Xft.antialias:	1
Xft.hinting:	1
Xft.autohint:	0
Xft.hintstyle:	hintslight
Xft.rgba:	rgb
Xft.lcdfilter:	lcddefault

st.font:        Hack:pixelsize=13:antialias=true:autohint=true
st.font2:       HackGen:pixelsize=13:antialias=true:autohint=true

! window padding
st.borderpx: 4

!- 0.1 to 0.9 = transparency
st.alpha: 0.94

*background: #101419
! *foreground: #b6beca

! Black + DarkGrey
